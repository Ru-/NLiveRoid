<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:background="#00000000"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">
<LinearLayout
android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:gravity="left"
	android:layout_gravity="left">

			<LinearLayout
			    android:id="@+id/ranking_linear"
android:layout_width="wrap_content"
	android:layout_height="fill_parent"
	android:orientation="vertical"
	android:visibility="gone"
			    >	<TextView
		android:gravity="center"
		android:textSize="25dp"
	    android:id="@+id/ranking_num"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text=""/>
	<ImageView
		android:gravity="center"
	    android:id="@+id/ranking_allow"
android:layout_width="wrap_content"
	android:layout_height="wrap_content" />
	<TextView
		android:gravity="center"
	    android:id="@+id/ranking_active"
android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:text=""/>
			</LinearLayout>
			<ImageView android:layout_gravity="left"
				android:id="@+id/community_thumbnail"
				android:layout_width="88.0dip" android:layout_height="88.0dip"
				android:background="@drawable/noimage"
				android:scaleType="centerCrop">
			</ImageView>
			<TableLayout
			    android:layout_width="fill_parent"
				android:layout_height="wrap_content">
				<TableRow>
					<TextView android:id="@+id/search_list_liveid"
						android:singleLine="true" android:layout_height="wrap_content"
						android:layout_width="fill_parent" android:text="放送ID"></TextView>
				</TableRow>
				<TableRow>
					<TextView android:id="@+id/search_list_communityid"
						android:singleLine="true" android:layout_height="wrap_content"
						android:layout_width="fill_parent" android:text="コミュID"></TextView>
				</TableRow>
				<TableRow>
					<LinearLayout android:id="@+id/search_list_tag_linear" android:layout_height="12dip"
						android:layout_width="fill_parent"></LinearLayout>
				</TableRow>
				<TableRow>
					<TextView android:id="@+id/search_list_livetitle"
						android:layout_height="wrap_content" android:layout_width="fill_parent"
						android:singleLine="true" android:text="放送タイトル"></TextView>
				</TableRow>
				<TableRow>
					<TextView android:id="@+id/search_list_starttime"
						android:layout_height="wrap_content" android:layout_width="fill_parent"
						android:text="開始時間"></TextView>
				</TableRow>
				<TableRow>
	<TableLayout
						android:layout_height="fill_parent" android:layout_width="fill_parent"
						android:stretchColumns="2">
						<TableRow>
					<TextView android:id="@+id/search_list_viewcount"
						android:layout_height="wrap_content" android:layout_width="wrap_content"
						android:gravity="left"
						android:text="来場"></TextView>
					<TextView android:id="@+id/search_list_rescount"
						android:layout_height="wrap_content" android:layout_width="wrap_content"
						android:gravity="left"
						android:text="コメ数"></TextView>
						</TableRow>
						</TableLayout>
				</TableRow>

			</TableLayout>
			</LinearLayout>
						<FrameLayout
						android:id="@+id/search_list_progress_frame"
						android:layout_height="fill_parent"
						android:layout_width="fill_parent">
<ProgressBar android:layout_width="88.0dip"
					android:layout_height="88.0dip" style="@android:style/Widget.ProgressBar"
					android:id="@+id/progressBar1">	</ProgressBar>
					</FrameLayout>
</FrameLayout>
